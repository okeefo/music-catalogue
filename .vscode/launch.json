{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Debug: Run main_window",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/main_window.py",
            "console": "integratedTerminal",
        },
        {
            "name": "Profile: main_window",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/main_window.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "-m",
                "cProfile",
                "-o",
                "profile.out"
            ]
        }
    ]
}